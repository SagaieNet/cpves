<h1>New domain</h1>

<% form_for(@domain) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :adminid %><br />
    <%= f.text_field :adminid %>
  </p>
  <p>
    <%= f.label :dnsname %><br />
    <%= f.text_field :dnsname %>
  </p>
  <p>
    <%= f.label :dnsname_idn %><br />
    <%= f.text_field :dnsname_idn %>
  </p>
  <p>
    <%= f.label :comment %><br />
    <%= f.text_field :comment %>
  </p>
  <p>
    <%= f.label :state %><br />
    <%= f.text_field :state %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', domains_path %>